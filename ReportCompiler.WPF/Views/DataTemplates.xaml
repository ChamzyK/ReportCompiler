<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ucvm="clr-namespace:ReportCompiler.WPF.ViewModels.UserControlViewModels"
                    xmlns:ucv="clr-namespace:ReportCompiler.WPF.Views.UserControls"
                    xmlns:dv="clr-namespace:ReportCompiler.WPF.Views.Dialogues"
                    xmlns:dvm="clr-namespace:ReportCompiler.WPF.ViewModels.Dialogues">
    
    <!--Dilaogue-->
    <DataTemplate DataType="{x:Type dvm:IncorrectReportsViewModel}">
        <dv:IncorrectReportsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type dvm:SuccessResultViewModel}">
        <dv:SuccessResultView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type dvm:InfoViewModel}">
        <dv:InfoView/>
    </DataTemplate>

    <!--UserControl-->
    <DataTemplate DataType="{x:Type ucvm:MenuViewModel}">
        <ucv:MenuView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ucvm:MetaDataViewModel}">
        <ucv:MetaDataView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ucvm:ReportsViewModel}">
        <ucv:ReportsView/>
    </DataTemplate>
    
</ResourceDictionary>